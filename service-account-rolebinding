  name: 'system:image-puller-2'
  namespace: lucee-app
subjects:
  - kind: ServiceAccount
    name: lucee-app
    namespace: lucee-app
roleRef:
  apiGroup: rbac.authorization.k8s.io
  kind: ClusterRole
  name: 'system:image-puller'